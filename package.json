{
  "name": "dalle",
  "version": "0.1.1",
  "description": "AI model drawing images for Node.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/node-dalle#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/node-dalle.git"
  },
  "bugs": "https://github.com/wobsoriano/node-dalle/issues",
  "keywords": [
    "dalle",
    "ai",
    "python",
    "images"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "main.py"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "npm run build-fast --dts-resolve",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "tsx playground.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "node-calls-python": "^1.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@types/node": "^18.0.6",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "tsup": "6.0.1",
    "tsx": "^3.4.3",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
